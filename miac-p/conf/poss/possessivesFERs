linksN		lkn		miacp.featgen.fer.WordNetLinksFER		OverridePos=NN
linksV		lkv		miacp.featgen.fer.WordNetLinksFER		OverridePos=VB
linksDV		lkv		miacp.featgen.fer.WordNetLinksFER		OverridePos=VB	DerivedInputPos=NN

